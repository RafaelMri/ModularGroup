   RowBox[{"{", 
    RowBox[{"it", ",", "t", ",", "n", ",", "transformations"}], "}"}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"it", "=", 
     RowBox[{"ModularGroupIterator", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"PSL2ZGrading", "@", 
         RowBox[{"Mat", "[", "#1", "]"}]}], "\[LessEqual]", 
        RowBox[{"PSL2ZGrading", "@", 
         RowBox[{"Mat", "[", "#2", "]"}]}]}], "&"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"n", "=", "0"}], ";", "\[IndentingNewLine]", 
    RowBox[{"transformations", " ", "=", " ", 
